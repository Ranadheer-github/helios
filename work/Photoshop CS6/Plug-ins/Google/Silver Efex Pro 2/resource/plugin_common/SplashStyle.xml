	<metric name="RightMargin" value="30"/>
	<metric name="TopMargin" value="11"/>
	<metric name="BottomMargin" value="12"/>
  </widget>

  <part name="SplashLineEdit">
    <state name="normal">
      <use template="LineEditSplash" border="2;2;2;4"/>
    </state>
    <state name="focused">
      <use state="normal"/>
    </state>
    <state name="disabled">
      <use template="LineEditSplash" border="2;2;2;4"/>
    </state>
  </part>
	<!-- remove -->   
  <part name="SplashScreenPhoneFont">
      <state name="normal">
	  <text font="SplashScreenPhoneFontF" margin="0;1;0;-1" flags="center;top" color="SSWhiteShadow" wrapmode="WrapWord"/>
	  <text font="SplashScreenPhoneFontF" margin="0;0;0;0" flags="center;top" color="SplashScreenFontColor" wrapmode="WrapWord"/>
      </state>
    </part>
   <part name="SplashScreenStandart">
