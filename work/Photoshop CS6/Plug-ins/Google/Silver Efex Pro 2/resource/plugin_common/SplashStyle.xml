<!-- remove -->
   <part name="SplashScreenBottom">
      <state name="normal">
	  <text font="SplashScreenBottomF" margin="0;0;0;0" flags="left;top" color="SplashScreenFontColor" wrapmode="WrapWord"/>
      </state>
    </part>
	<part name="SplashScreenButton">
		<state name="normal">
			<use template="GoogleBtn-Norm"/>
			<text font="SplashScreenButtonF" margin="0;-1;0;0" flags="center;middle" color="DialogText" />
		</state>
		<state name="normal.hover">
			<use template="GoogleBtn-Hover"/>
			<text font="SplashScreenButtonF" margin="0;-1;0;0" flags="center;middle" color="DialogText" />
		</state>
		<state name="normal.pressed">
			<use template="GoogleBtn-Down"/>
			<text font="SplashScreenButtonF" margin="0;-1;0;0" flags="center;middle" color="DialogText" />
		</state>
		<state name="normal.pressed.hover">
			<use state="normal.pressed"/>
		</state>
		<state name="normal.focused">
			<use state="normal"/>
		</state>
		<state name="normal.focused.hover">
			<use state="normal.hover"/>
		</state>
		<state name="activated">
			<use state="normal.pressed"/>
		</state>
		<state name="activated.hover">
			<use state="activated"/>
		</state>
		<state name="activated.pressed">
			<use state="activated"/>
		</state>
		<state name="activated.focused">
			<use state="activated"/>
		</state>
		<state name="activated.focused.hover">
			<use state="activated.hover"/>
		</state>
		<state name="activated.pressed.hover">
			<use state="activated.hover"/>
		</state>
	</part>
	<part name="SplashScreenLabelButton">
		<substate>
			<state name="normal">
				<text font="SplashScreenStandartF" margin="0;1;0;-1" flags="left;middle" color="SSWhiteShadow" />
				<text font="SplashScreenStandartF" margin="0;0;0;0" flags="left;middle" color="SplashScreenFontColor" />
			</state>
			<state name="activated">
				<text font="SplashScreenStandartF" margin="0;1;0;-1" flags="left;middle" color="SSWhiteShadow" />
				<text font="SplashScreenStandartF" margin="0;0;0;0" flags="left;middle" color="SplashScreenFontColor" />
			</state>
			<state name="activated.disabled">
				<text font="SplashScreenStandartF" margin="0;1;0;-1" flags="left;middle" color="SSWhiteShadow" />
				<text font="SplashScreenStandartF" margin="0;0;0;0" flags="left;middle" color="SplashScreenFontColor" />
			</state>
		</substate>
		<substate>
			<state name="pressed"></state>
			<state name="focused"></state>
		</substate>
		<substate>
			<state name="hover">
				<text font="SplashScreenStandartF" margin="0;1;0;-1" flags="left;middle" color="SSBlackShadow" />
				<text font="SplashScreenStandartF" margin="0;0;0;0" flags="left;middle" color="SplashScreenFontColor" />
			</state>
		</substate>
		<state name="normal.disabled" tinting="1;1;1;0.6">
			<text font="SplashScreenStandartF" margin="0;1;0;-1" flags="left;middle" color="SSWhiteShadow" />
			<text font="SplashScreenStandartF" margin="0;0;0;0" flags="left;middle" color="SplashScreenFontColor" />
		</state>
	</part>
