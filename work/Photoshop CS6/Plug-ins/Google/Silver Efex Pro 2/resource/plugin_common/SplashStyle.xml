	<part name="SplashAcknowledgmentsLabel">
        <state name="normal">
            <text font="SplashScreenBottomUnderlineF" margin="0;1;0;-1" flags="left;top" color="SSWhiteShadow"/>
            <text font="SplashScreenBottomUnderlineF" margin="0;0;0;0" flags="left;top" color="SplashScreenFontColor"/>
        </state>
        <state name="normal.hover">
            <text font="SplashScreenBottomUnderlineF" margin="0;-1;0;1" flags="left;top" color="SSBlackShadow"/>
            <text font="SplashScreenBottomUnderlineF" margin="0;0;0;0" flags="left;top" color="SplashScreenHighlightFontColor"/>
        </state>
        <state name="normal.pressed">
