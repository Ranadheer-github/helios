    <metric name="RightMargin" value="5"/>
    <metric name="TopMargin" value="2"/>
    <metric name="BottomMargin" value="2"/>
    <string name="FontStyle" value="Bold" />
  </widget>
  <widget name="SplashStyleSwitchButton">
    <metric name="LeftMargin" value="0"/>
    <metric name="RightMargin" value="5"/>
    <metric name="TopMargin" value="2"/>
    <metric name="BottomMargin" value="2"/>
